{
  "name": "course-work",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next dev",
    "prod": "env-cmd -f .env.prod next dev",
    "build": "next build",
    "start": "env-cmd -f .env.dev next start",
    "lint": "next lint",
    "db:generate": "env-cmd -f .env.dev prisma generate",
    "db:migrate": "env-cmd -f .env.dev prisma migrate dev",
    "db:migrate:prod": "env-cmd -f .env.prod prisma migrate dev",
    "db:push": "env-cmd -f .env.dev prisma db push",
    "db:studio": "env-cmd -f .env.dev prisma studio",
    "db:seed": "env-cmd -f .env.dev ts-node prisma/db-seed.ts",
    "db:seed:prod": "env-cmd -f .env.prod ts-node prisma/db-seed.ts",
    "stripe:listen": "env-cmd -f .env.dev stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@auth/prisma-adapter": "^2.0.0",
